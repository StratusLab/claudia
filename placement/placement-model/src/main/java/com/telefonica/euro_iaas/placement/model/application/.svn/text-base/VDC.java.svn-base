package com.telefonica.euro_iaas.placement.model.application;

import java.util.Set;


/**
 * Virtual Data Center
 * 
 * @author Javier de la Puente Alonso
 *
 */
public class VDC{
	
	private String name;
	private Set<ServiceApplication> serviceApplications;

	/**
	 * 
	 */
	public VDC() {
		super();
	}

	public Set<ServiceApplication> getServiceApplications() {
		return serviceApplications;
	}

	public void setServiceApplications(Set<ServiceApplication> serviceApplications) {
		this.serviceApplications = serviceApplications;
	}

	/**
	 * @return the name
	 */
	public String getName() {
		return name;
	}

	/**
	 * @param name the name to set
	 */
	public void setName(String name) {
		this.name = name;
	}

	/* (non-Javadoc)
	 * @see java.lang.Object#toString()
	 */
	@Override
	public String toString() {
		return "VDC [name=" + name + ", serviceApplications="
				+ serviceApplications + "]";
	}


}
